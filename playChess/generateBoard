void generateBoard(char pieces[][])
{
  //Generates pieces for start of chess game.
  
  pieces = [['♜','♞','♝','♛','♚','♝','♞','♜'],
           ['♟','♟','♟','♟','♟','♟','♟','♟'],
           ['.','.','.','.','.','.','.','.'],
           ['.','.','.','.','.','.','.','.'],
           ['.','.','.','.','.','.','.','.'],
           ['.','.','.','.','.','.','.','.'],
           ['♙','♙','♙','♙','♙','♙','♙','♙'],
           ['♖','♘','♗','♕','♔','♗','♘','♖']]
}
